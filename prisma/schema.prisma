generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Vote {
  id           String   @id @default(cuid())
  createdAt    DateTime @default(now())
  votedFor     Int
  votedAgainst Int
}
